{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/frontend",
            "breakOnLoad": true,
            "sourceMaps": true,
            "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"]
        }
    ]
}
